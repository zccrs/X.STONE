class Manager
{
    SLOT(QString createClient());
    SLOT(destroyClient(const QString &id));
};

class Client
{
    SLOT(QString createSurface());
    SIGNAL(ping());
    SLOT(pong());
};

#include <QRect>
class Surface
{
    PROP(QRect geometry READWRITE);
    PROP(bool visible READWRITE);
    SLOT(destroy());
}
